{
  "manifest_version": 3,
  "minimum_chrome_version": "100",
  "name": "ChromeOS AutoStart",
  "version": "5.1.1",
  "description": "Run Linux commands at ChromeOS startup.",
  "homepage_url": "https://github.com/supechicken/ChromeOS-AutoStart",
  "author": "supechicken666@gmail.com",
  "icons": {
    "192": "img/icon-192.png",
    "512": "img/icon-512.png"
  },
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  },
  "offline_enabled": true,
  "options_page": "html/options_page.html",
  "options_ui": {
    "page": "html/options_page.html",
    "open_in_tab": false
  },
  "permissions": [ "terminalPrivate", "storage", "notifications" ],
  "key": ""
}
